<h1>Where to Buy Near <%= @zip %>
<% @stores.first(10).each do |store| %>

  <h3><%= store["name"] %></h3>
    <address>
      <%= store["address"] %>
      <%= store["city"] %>, <%= store["state"] %>
      <%= store["number"] %>
    </address>
    <a href="<%=store["url"]%>">Website</a>

<% end %>
